<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">



    <CheckBoxPreference
        android:defaultValue="false"
        android:key="rides"
        android:title="@string/pref_title_rides"
        />
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="admin"
        android:title="@string/pref_title_dev" />
    <ListPreference
        android:defaultValue="360"
        android:entries="@array/notificationTimeOptions"
        android:entryValues="@array/notificationTimeValues"
        android:key="eventupdatetime"
        android:summary="%s"
        android:title="Event Update Interval" />
    <ListPreference
        android:defaultValue="360"
        android:entries="@array/notificationTimeOptions"
        android:entryValues="@array/notificationTimeValues"
        android:key="notifiybeforetime"
        android:summary="%s"
        android:title="Notify Before Event" />
    <Preference
        android:key="loadpackbutton"
        android:summary="Opens a new dialog"
        android:title="Load Chapter Pack"
        />

</PreferenceScreen>
